  g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
  s.parentNode.insertBefore(g,s)}(document,'script'));</script> -->

  <script type="text/javascript">
    var themeTitle = "Studio Lite Theme";
    var url = "http://studio-lite-theme.ghost.io";
    (function() {
        var pxu = document.createElement('script'); pxu.type = 'text/javascript'; pxu.async = true; pxu.src = 'http://cdn.pixelunion.net/customize/pxucm.js';
        var isDemo = (window.location.href.indexOf(url) >= 0);
        (isDemo) && ((document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(pxu));
    })();
  </script>
</body>
</html>
