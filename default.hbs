<head>

  <meta charset="utf-8" />
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible" />
  <meta content="{{meta_description}}" name="description" />
  <meta content="width=device-width, initial-scale=1" name="viewport" />
  <link href="{{asset '/favicon.ico'}}" rel="shortcut icon" />
  <link href="/rss" rel="alternate" type="application/rss+xml" />
  <title>{{meta_title}}</title>

  <!-- Facebook OG -->
  <meta content="blog" property="og:type" />
  <meta content="{{meta_description}}" property="og:description" />
  <meta content="{{@blog.logo}}" property="og:image" />

  {{#foreach posts}}
    <meta content="article" property="og:type" />
    <meta content="{{url absolute='true'}}" property="og:url" />
    <meta content="{{encode excerpt words='30'}}" property="og:description" />
    <meta content="{{encode title}}" property="og:title" />
    <meta content="{{@blog.url}}/{{@blog.logo}}" property="og:image" />
  {{/foreach}}

  <link rel="stylesheet" type="text/css" href="{{asset 'css/style.css'}}" />
  <link rel="stylesheet" type="text/css" href="{{asset 'css/prism.css'}}" />
  <link href='//fonts.googleapis.com/css?family=Lato:300,400italic,600italic,700italic,400,700,600' rel='stylesheet' type='text/css'>
  <link href='//fonts.googleapis.com/css?family=Libre+Baskerville:400' rel='stylesheet' type='text/css'>

  {{ghost_head}}
</head>

<body class="{{body_class}}">

  <header class="site-header {{#if @blog.cover}}sidebar-background-image has-background{{/if}}" id="navigation">

    {{#if @blog.cover}}
      <div class="header" {{#if @blog.cover}}style="background-image: url({{@blog.cover}})"{{/if}}></div>
      <div class="header-overlay"></div>
    {{/if}}

    {{#if @blog.logo}}
      <h1 class="logo">
        <a href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="Logo" /></a>
      </h1>
    {{else}}
      <h1 class="no-logo"><a href="/">{{@blog.title}}</a></h1>
    {{/if}}

    <div class="blog-description rte">
      {{@blog.description}}
    </div>

    <div class="closed" id="nav-wrapper">
      <div class="button-wrap">
        <div id="nav-button">
          <div class="icon"></div>
        </div>
      </div>
      <ul id="nav-list">
        {{> custom-links}}
      </ul>
    </div>

  </header>

  {{{body}}}

  <footer class="site-footer {{#if @blog.cover}}header-has-background{{/if}}">
    <div class="social-icons clearfix">
      {{> social-links}}
    </div>
    <p> 2008 - 2014 AlphaSights Ltd. <br>All Rights Reserved.</p>
<!--     <p>Subscribe via <a href="{{@blog.url}}/rss/">RSS</a></p>
    <p>Proudly published with <a href="http://tryghost.org">Ghost</a></p> -->
  </footer>

  {{ghost_foot}}

  <script type="text/javascript" src="{{asset 'js/script.js'}}"></script>
  <script type="text/javascript" src="{{asset 'js/prism.js'}}"></script>

  <!-- Google Analytics -->
  <!-- <script type="text/javascript">var _gaq=[['_setAccount','TRACKING ID'],['_trackPageview']];
